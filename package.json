{
	"name": "vscode-azure-devops",
	"displayName": "vscode-azure-devops",
	"description": "Azure Devops Pull Request Monitor",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:pullRequests",
		"onView:settings"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "azure-devops",
					"title": "Azure Devops",
					"icon": "media/AzureDevops.svg"
				}
			]
		},
		"views": {
			"azure-devops": [
				{
					"id": "pullRequests",
					"name": "Pull Requests"
				}
			]
		},
		"commands": [
			{
				"command": "pullRequests.refresh",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "pullRequests.refresh",
					"when": "view == pullRequests",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"@types/proxyquire": "^1.3.28",
		"@types/sinon": "^7.5.1",
		"@types/vscode": "^1.41.0",
		"azure-devops-node-api": "^6.6.2",
		"glob": "^7.1.6",
		"mocha": "^7.0.0",
		"proxyquire": "^2.1.3",
		"sinon": "^8.0.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.3.0"
	}
}
